<div class="card border border-primary" style="max-width:400px">
  <div class="card-body">
    <h4 class="card-title text-primary">{{system.model}}</h4>
    <p class="card-text">
   <table class="table table-hover">
  <tbody>
    <tr>
      <th scope="row">Manufacturer</th>
      <td>{{system.manufacturer}}</td>
    </tr>
<tr>
<th scope="row">CPU</th>
<td>{{cpu.manufacturer}} {{cpu.brand}}</td>
</tr>
<tr>
<th scope="row">Vendor</th>
<td>{{cpu.vendor}}</td>
</tr>
<tr>
<th scope="row">Manufacturer</th>
<td>{{cpu.family}} {{cpu.socket}}</td>
</tr>
  </tbody>
</table>
    </p>
  </div>
  <div class="d-flex justify-content-between align-items-center flex-wrap p-2">
    <div>
    </div>
  </div>
</div>

